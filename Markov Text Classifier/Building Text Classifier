    "#### Create a mapping from unique word to unique integer index"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "3f9ba1f7",
   "metadata": {},
   "outputs": [],
   "source": [
    "current_idx = 0\n",
    "word2idx = {'<unk>':0} #some words in test set maybe not in train set\n",
    "\n",
    "for text in inputs_train:\n",
    "    tokens = text.split()\n",
    "    for token in tokens:\n",
    "        if token not in word2idx:\n",
    "            word2idx[token] = current_idx\n",
    "            current_idx += 1"
   ]
